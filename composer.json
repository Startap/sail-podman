{
    "name": "startap/sail-podman",
    "description": "Podman compatibility layer to @laravel/sail application.",
    "keywords": ["laravel", "docker", "podman"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/startap/sail-podman/issues",
        "source": "https://github.com/startap/sail-podman"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        },
        {
            "name": "Andr√© Paul Grandsire",
            "email": "andre@startap.dev.br"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/console": "^9.0|^10.0|^11.0",
        "illuminate/contracts": "^9.0|^10.0|^11.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "symfony/yaml": "^6.0|^7.0"
    },
    "bin": [
        "bin/sail"
    ],
    "autoload": {
        "psr-4": {
            "Laravel\\Sail\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Laravel\\Sail\\SailServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "phpstan/phpstan": "^1.10"
    }
}
